#name ".//test_files//hairyscary.cl"


#3 CLASS
Foo inherits Bazz {
     a : Razz <- case self of
		      n : Razz #5 DARROW
 (new Bar);
		      n : Foo #6 DARROW
 (new Razz);
		      n : Bar #7 DARROW
 n;
   	         esac;

     b : Int <- a.doh() + g.doh() + doh() + printh();

     doh() : Int { (let i : Int <- h in { h <- h + 2; i; } ) };

};

#16 CLASS
Bar inherits Razz {

     c : Int <- doh();

     d : Object <- printh();
};


#24 CLASS
Razz inherits Foo {

     e : Bar <- case self of
		  n : Razz #27 DARROW
 (new Bar);
		  n : Bar #28 DARROW
 n;
		esac;

     f : Int <- a@Bazz.doh() + g.doh() + e.doh() + doh() + printh();

};

#35 CLASS
Bazz inherits IO {

     h : Int <- 1;

     g : Foo  <- case self of
		     	n : Bazz #40 DARROW
 (new Foo);
		     	n : Razz #41 DARROW
 (new Bar);
			n : Foo  #42 DARROW
 (new Razz);
			n : Bar #43 DARROW
 n;
		  esac;

     i : Object <- printh();

     printh() : Int { { out_int(h); 0; } };

     doh() : Int { (let i: Int <- h in { h <- h + 1; i; } ) };
};


#54 CLASS
Main {
  a : Bazz <- new Bazz;
  b : Foo <- new Foo;
  c : Razz <- new Razz;
  d : Bar <- new Bar;

  main(): String { "do nothing" };

};





